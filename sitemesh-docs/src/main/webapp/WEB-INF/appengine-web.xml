<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <application>sitemesh-docs</application>

  <version>dev</version>

  <!-- Don't treat any of the files as static - so Servlet engine (and SiteMesh) can handle them. -->
  <static-files> 
    <exclude path="**"/> 
  </static-files>

</appengine-web-app>
